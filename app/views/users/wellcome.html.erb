<h1 align="center">wellcome to Bookmyshow</h1>
<h1>Sign In</h1>
  <%= form_with url: login_path, method: :post do |f| %>
    <p>
    <%= f.label 'email:'%><br>
    <%= f.text_field :email, id: 'email' %>
    </p>
    <p>
    <%= f.label 'password:'%><br>
    <%= f.password_field :password, id: 'password' %>
    </p>
    <p>
    <%= f.submit 'Log In' %>
    </p>
  <% end %>

<%= button_to "Sign up", new_user_path, method: :get %>



